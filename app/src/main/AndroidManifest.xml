<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.musicalstructureapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".Albums"
            android:label="@string/albums"/>
        <activity android:name=".Artists"
            android:label="@string/artists"/>
        <activity android:name=".Genres"
            android:label="@string/genres"/>
        <activity android:name=".Playlists"
            android:label="@string/playlists"/>
        <activity android:name=".Songs"
            android:label="@string/songs"></activity>
    </application>

</manifest>